#include <stdio.h>

int main ()
{
  int a, b, c, max;
  printf ("Enter 3 number:-");
  scanf ("%d%d%d", &a, &b, &c);
  if (a > b)
    {
        if(a>c)
      max = a;
      else

      max = c;
    }
  else
    {
      if (b > c)
	max = b;
      else
	max = c;
    }
  printf ("Max of %d,%d,%dnumber=%d", a, b, c, max);
  return 0;
}
