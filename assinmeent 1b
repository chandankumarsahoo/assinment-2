#include <stdio.h>

int
main ()
{
  int marks;
  char grade;
  printf ("Enter The Mark Of Student (Out Of 100):-  ");
  scanf ("%d", &marks);
  marks = marks / 10;
  switch (marks)
    {
    case 10:
    case 9:

      grade = 'O';
      break;
    case 8:

      grade = 'E';
      break;
    case 7:

      grade = 'A';
      break;
    case 6:

      grade = 'B';
      break;
    case 5:

      grade = 'C';
      break;
    case 4:

      grade = 'D';
      break;
    default:


      grade = 'F';
      break;
    }

  printf ("Grade Of Student:- %c", grade);
  return 0;
}
